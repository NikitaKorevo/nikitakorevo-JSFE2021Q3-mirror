{
    "plugins": ["prettier", "import"],
    "extends": [
        "plugin:prettier/recommended",
        "prettier",
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "parser": "airbnb-typescript/base",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off"
    }
}
